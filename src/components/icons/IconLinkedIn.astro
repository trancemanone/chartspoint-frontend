---
/**
 * IconLinkedIn - LinkedIn logo
 * Represents LinkedIn social media
 */
interface Props {
  class?: string;
  size?: number;
  strokeWidth?: number;
}

const { class: className = '', size = 24, strokeWidth = 1.75 } = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  fill="none"
  stroke="currentColor"
  stroke-width={strokeWidth}
  stroke-linecap="round"
  stroke-linejoin="round"
  class={className}
  width={size}
  height={size}
  aria-hidden="true"
>
  <rect x="2" y="2" width="20" height="20" rx="2" />
  <line x1="8" y1="11" x2="8" y2="16" />
  <line x1="8" y1="8" x2="8" y2="8.01" />
  <path d="M12 16v-5c0-1.5 1-2 2-2s2 .5 2 2v5" />
</svg>
